<h1> Sign Up </h1>
<%= form_for :user, url: users_path do |f| %>
	<%= f.label :UserName %><br/>
	<%= f.text_field :username %><br/>
	<%= f.label :Password %><br/>
	<%= f.password_field :password %><br/>
	<%= f.label :ConfirmPassword %><br/>
	<%= f.password_field :password_confirmation %><br/>
	<%= f.label :EmailID %><br/>
	<%= f.text_field :email %><br/>
	<%= f.submit %>
<% end %>
