<h1> ADD NEW CONTACT </h1>
<%= form_for :contact,url: contacts_path do |f| %>
<p>
	<p>
		<%= f.label :FirstName %><br/>
		<%= f.text_field :firstname %><br/>
	</p>
	<p>
		<%= f.label :LastName %><br/>
		<%= f.text_field :lastname %><br/>
	</p>
	<p>
		<%= f.label :Email %><br/>
		<%= f.text_field :email %><br/>
	</p>
	<p>
		<%= f.label :MobileNumber %><br/>
		<%= f.text_field :mobile %><br/>
	</p>
	<p>
		<%= f.label :Address %><br/>
		<%= f.text_area :address %><br/>
	</p>
	<p>
		<%= f.label :UploadPhoto %><br/>
		<%= f.file_field :photo %>
	</p>
	<%= f.submit %>
</p>
<% end %>